import java.util.*;

public class Main{
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);
        int total=0, i=0;
        int n = s.nextInt(); // receive n

        for(i=1; i<n; i++){
            total += 1+(i*2);
            // System.out.println("["+(1+(i*2))+"]  "+i);
            // System.out.println("total "+total);
            if(n>total) continue;
            else break; 
        }
    System.out.println(i);
    }
}
